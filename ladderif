package automaction;

import java.util.Scanner;

import javax.swing.plaf.synth.SynthOptionPaneUI;

public class ladderif {
	public static void main(String[] args) {
		int rno,s1,s2,s3,total;
		String sname;
		double per;
		
		Scanner sc=new Scanner(System.in);
		
		System.out.println("Enetr Student Name : ");
		sname=sc.next();
		System.out.println("Enetr Roll No: ");
		rno=sc.nextInt();
		System.out.println("Enetr Marks Of Subject 1 : ");
		s1=sc.nextInt();
		System.out.println("Enetr Mark Of Subject 2 : ");
		s2=sc.nextInt();
		System.out.println("Enter Mark Of Subject 3 : ");
		s3=sc.nextInt();
		
		total=s1+s2+s3;
		per=total/3;
		
		System.out.println("Student Name : "+sname);
		System.out.println("Roll No : "+rno);
		System.out.println("Total : "+total);
		System.out.println("Percentage :"+per);
		
		if(per>=70) 
		{
			System.out.println("Distinaction");
		}
		else if(per>=50) 
		{
			System.out.println("First Class");
		}
		else if(per>=50) 
		{
			System.out.println("Second Class");
		}
		else if(per>=40)
		{
			System.out.println("Pass Class");
		}
		else
		{
		     System.out.println("Faill");	
		}
	}
}
