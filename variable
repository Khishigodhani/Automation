/*
 *  primitive Data Type:
 *  
 *  1. byte -1 byte  Byte -Wrapper
 *  2. short - 2 byte Short
 *  3. int - 4 byte Integer
 *  4. long - 8 byte Long
 *  
 *  5. float - 4 byte Float
 *  6. double - 8 byte Double
 *  
 *  7. char - 1 byte Character
 *  8. boolean - true/false Boolean
 *  
 *  variable It is data name & is used to store the data value.
 *  
 *  1. Must start with an alphabets.
 *  2. Upper case & lower case are significant.
 *  ASCII: American standard code for Information Interchange 
 *  A-Z: 65 to 90
 *  a-z: 97 to 122
 *  3. keyword is not allowed as a variable name
 *  4. Space is not allowed between variable name.
 */

package automaction;
 
import java.util.Scanner;

public class variable {
 
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int x,y,z;
		System.out.print("Enetr X :");
		x=sc.nextInt();
		System.out.print("Enetr Y :");
		y=sc.nextInt();
		System.out.print("Enetr Z :");
		z=sc.nextInt();
		z=x+y;
		System.out.println("Addition : "+y);
	}
}
